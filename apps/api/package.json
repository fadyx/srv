{
	"name": "api",
	"version": "0.0.0",
	"private": true,
	"scripts": {
		"build": "yarn tsc  && yarn tsc-alias",
		"bundle": "node build.js",
		"start": "node src/index.js",
		"clean": "rm -rf dist",
		"check:types": "yarn tsc --noEmit",
		"dev:watch": "dotenv -e ../.env -- nodemon",
		"start:dev": "node dist/index.js",
		"dev": "yarn nodemon --inspect -r tsconfig-paths/register src/index.ts",
		"lint": "tsc --noEmit && TIMING=1 eslint \"src/**/*.ts*\"",
		"test": "jest --detectOpenHandles"
	},
	"dependencies": {
		"express": "^4.18.2",
		"morgan": "^1.10.0"
	},
	"devDependencies": {
		"@jest/globals": "^29.4.1",
		"@types/express": "^4.17.17",
		"@types/morgan": "^1.9.4",
		"@types/node": "^18.11.18",
		"@types/supertest": "^2.0.12",
		"esbuild": "^0.17.8",
		"eslint": "^8.33.0",
		"eslint-config-node": "*",
		"jestconfig": "*",
		"nodemon": "^2.0.20",
		"supertest": "^6.3.3",
		"tsc-alias": "^1.8.2",
		"tsconfig": "*",
		"typescript": "^4.9.5"
	}
}
