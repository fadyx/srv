{
	"name": "api",
	"version": "0.0.0",
	"private": true,
	"scripts": {
		"build": "yarn tsc  && yarn tsc-alias",
		"bundle": "rm -fr dist && node build.js",
		"run:prod": "node dist/src/index.js",
		"run:dev": "node -r ts-node/register --inspect=0.0.0.0:9229 -r tsconfig-paths/register -r source-map-support/register src/index.ts",
		"prod": "yarn run:prod",
		"dev": "yarn nodemon",
		"test": "jest --detectOpenHandles",
		"lint:check": "yarn tsc --noEmit && TIMING=1 yarn eslint .",
		"lint:fix": "yarn lint:check --fix",
		"clean": "rm -rf dist"
	},
	"dependencies": {
		"express": "^4.18.2",
		"morgan": "^1.10.0"
	},
	"devDependencies": {
		"@jest/globals": "^29.4.1",
		"@types/express": "^4.17.17",
		"@types/morgan": "^1.9.4",
		"@types/node": "^18.11.18",
		"@types/supertest": "^2.0.12",
		"dotenv-cli": "^7.0.0",
		"esbuild": "^0.17.8",
		"eslint": "^8.33.0",
		"eslint-config-node": "*",
		"jestconfig": "*",
		"nodemon": "^2.0.20",
		"supertest": "^6.3.3",
		"tsc-alias": "^1.8.2",
		"tsconfig": "*",
		"typescript": "^4.9.5"
	}
}
