{
	"$schema": "https://json.schemastore.org/tsconfig",
	"extends": "./base.tsconfig.json",
	"display": "node",
	"compilerOptions": {
		"lib": ["ES2021"],
		"module": "CommonJS",
		"target": "ES2021",
		"moduleResolution": "node",
		"resolveJsonModule": true,
		"strict": true,
		"noEmit": false,
		"sourceMap": true,
		"esModuleInterop": true,
		"skipLibCheck": true,
		"forceConsistentCasingInFileNames": true
	}
}
