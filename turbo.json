{
	"$schema": "https://turbo.build/schema.json",
	"globalDependencies": ["**/.env.local"],
	"globalEnv": [],
	"pipeline": {
		"build": {
			"dependsOn": ["^build"],
			"outputs": ["dist/**"],
			"env": []
		},
		"test": {
			"outputs": ["coverage/**"],
			"dependsOn": []
		},
		"lint": {
			"outputs": [],
			"dependsOn": ["^build"]
		},
		"dev": {
			"cache": false,
			"persistent": true
		},
		"clean": {
			"cache": false
		},
		"lint:staged": {
			"cache": false,
			"dependsOn": ["^build"]
		}
	}
}
